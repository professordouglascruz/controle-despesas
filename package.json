{
  "name": "expense-tracker",
  "version": "1.0.0",
  "description": "Sistema de lançamento de despesas com categorias, estabelecimentos e lançamentos",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects unit-tests",
    "test:repos": "jest --selectProjects repository-tests",
    "test:e2e": "jest --selectProjects e2e-tests",
    "test:sequential": "jest --runInBand --detectOpenHandles",
    "migrate": "node src/database/migrate.js",
    "seed": "node src/database/seedData.js",
    "seed:force": "node src/database/seedData.js --force",
    "setup": "npm run migrate && npm run seed",
    "reset": "npm run migrate && npm run seed:force",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "keywords": ["expense", "tracker", "despesas", "categorias"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "@types/jest": "^29.5.5",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/morgan": "^1.9.0"
  }
}